sandboxed: false
start: manual
version: 3.0.1

executables:
{
  mtkSetup = ( upload )
}

processes:
{
  run:
  {
    ( mtkSetup )
  }
  faultAction: restartApp
  envVars:
  {
    LE_LOG_LEVEL = DEBUG
  }
}

bindings:
{
  mtkSetup.upload.mtRst -> gpioService.le_gpioPin2
  mtkSetup.upload.mtBootstrap -> gpioService.le_gpioPin24
}
